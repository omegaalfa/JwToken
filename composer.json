    "type": "library",
    "license": "MIT",
    "keywords": [
        "jwt",
        "jwt.io",
        "autenticação",
        "token",
        "json-web-token", 
        "auth", 
        "authentication", 
        "token",
        "php8",
        "login",
        "security"
    ],
    "authors": [
        {
            "name": "wrt",
            "email": "webdesenvolver.agenda@gmail.com"
        }
    ],
    "require": {
        "php": "^8.0",
        "psr/http-message": "^1.1"
    },
    "autoload": {
        "psr-4": {
            "omegaalfa\\jwtoken\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "tests\\": "tests/"
        }
    },
   "require-dev": {
    "phpunit/phpunit": "^9.0"
  },
  "scripts": {
    "test": "phpunit"
  }
}
