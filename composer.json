{
  "name": "omegaalfa/jwtoken",
  "description": "A small, security-focused JWT library for PHP 8.4+ supporting HS256/384/512 and RS256.",
  "type": "library",
  "license": "MIT",
  "keywords": [
    "jwt",
    "json-web-token",
    "jwt.io",
    "authentication",
    "security",
    "hs256",
    "hs384",
    "hs512",
    "rs256",
    "php8"
  ],
  "authors": [
    {
      "name": "wrt",
      "email": "webdesenvolver.agenda@gmail.com"
    }
  ],
  "require": {
    "php": "^8.4",
    "psr/http-message": "^1.1 || ^2.0",
    "ext-openssl": "*"
  },
  "autoload": {
    "psr-4": {
      "Omegaalfa\\Jwtoken\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "tests\\": "tests/"
    }
  },
  "require-dev": {
    "phpunit/phpunit": "^9.5 || ^10.0",
    "phpstan/phpstan": "^1.11 || ^2.0"
  },
  "scripts": {
    "test": "phpunit"
  }
}
